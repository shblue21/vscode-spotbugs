{
  "name": "vscode-spotbugs",
  "displayName": "Spotbugs",
  "description": "Visual Studio Code extension for Spotbugs",
  "version": "0.0.1",
  "publisher": "JIHUN KIM",
  "icon": "images/spotbugs_logo.png",
  "author": {
    "name": "JIHUN KIM",
    "email": "jihunkimkw@gmail.com"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shblue21/vscode-spotbugs"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:spotbugs.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "spotbugs.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "extensionDependencies": [
    "redhat.java"
  ],
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
